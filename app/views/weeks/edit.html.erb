<h3>Week Form</h3>

<%= form_for(@week) do |f| %>
  <label>New Week!</label>
  <br></br>

  <label>Day and Date:</label>
  <%= f.text_field :week_day_date %><br>

  <label>Big Win:</label>
  <%= f.text_field :week_big_win_1 %><br>

  <label>Improvement 1:</label>
  <%= f.text_field :week_improvement_1 %><br>
 
  <%= f.submit %>
<% end %>